{
  "eslint.alwaysShowStatus": true,
  "editor.formatOnSave": true,
  "cucumberautocomplete.onTypeFormat": true,
  "cucumberautocomplete.steps": ["src/test/*.steps.js", "src/test/*.steps.ts"],
  "cucumberautocomplete.skipDocStringsFormat": true,
  "cucumberautocomplete.syncfeatures": "src/test/**/*.feature",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "**/lib": true,
    "**/test-results": true
  },
  "standard.usePackageJson": true,
  "standard.workingDirectories": [],
  "editor.rulers": [120],
  "commit-message-editor.view.defaultView": "form",
  "commit-message-editor.staticTemplate": [
    "<type>[(scope)]: <description>",
    "",
    "[optional body]",
    "",
    "[[BREAKING CHANGE: ]optional footer(s)]"
  ],
  "cSpell.words": [
    "ationship",
    "babelify",
    "browserstack",
    "bufferutil",
    "Builtins",
    "chromedriver",
    "codacy",
    "codeclimate",
    "commitlint",
    "conventionalcommits",
    "cookied",
    "corejs",
    "curied",
    "dependabot",
    "Edsger",
    "funkyness",
    "geckodriver",
    "github's",
    "godban",
    "HATEOAS",
    "huzzah",
    "hyperwizard",
    "instanceof",
    "invision",
    "invocables",
    "iphone",
    "istanbuljs",
    "jscpd",
    "jsonpointer",
    "lcov",
    "lokijs",
    "markdownlint",
    "mkdir",
    "mountainpass",
    "npmjs",
    "parameterised",
    "prerelease",
    "refs",
    "rels",
    "rxts",
    "saucelabs",
    "searchables",
    "shellcheck",
    "snyk",
    "standardjs",
    "styfle",
    "tada",
    "tsflow",
    "turnstyle",
    "vars",
    "waychaser",
    "waychaser's",
    "webdriver",
    "webpacking",
    "Wellerman",
    "windyroad"
  ],
  "cSpell.language": "en-GB",
  "cSpell.enableFiletypes": ["feature"],
  "cucumberautocomplete.gherkinDefinitionPart": "@(given|when|then|step)\\(",
  "cucumberautocomplete.stepsInvariants": true,
  "cucumberautocomplete.strictGherkinValidation": true
}
